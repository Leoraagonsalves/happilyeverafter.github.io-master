        <!-- form handling -->
        <form action="/" method="POST">
            <table class="table">
                <thead class="thead-dark">
                    <tr>
                        
                        <th>Customer Fullname</th>
                        <th>Phone Number</th>
                        <th>Email</th>
                        <th>Wedding Date</th>
                        <th>Action</th>
                    
                    </tr>
                </thead>
                <tbody>
                    
                    <% for(var i = 0; i < vendor.length; i++) { %>
                        <% if (req.user.username==vendor[i].vendorcontact) { %>
                     
                          
                        <tr>
                            
                            <td><%=vendor[i].fullname %></td>
                            <td><%= vendor[i].num %></td>
                            <td><%=vendor[i].email%></td>
                            <td><%=vendor[i].date%></td>
                            
                           
                            <td>
                                
                                <a class="btn border-shadow delete" data-id=<%= vendor[i].id%> >
                                    <span class="text-gradient"><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-x-lg" viewBox="0 0 16 16">
                                        <path fill-rule="evenodd" d="M13.854 2.146a.5.5 0 0 1 0 .708l-11 11a.5.5 0 0 1-.708-.708l11-11a.5.5 0 0 1 .708 0Z"/>
                                        <path fill-rule="evenodd" d="M2.146 2.146a.5.5 0 0 0 0 .708l11 11a.5.5 0 0 0 .708-.708l-11-11a.5.5 0 0 0-.708 0Z"/>
                                      </svg></span>
                                </a>
                            </td>
                        </tr>
                        <% } %>
                        <% } %>
                </tbody>
            </table>
        </form>
